{
    "name": "secrets",
    "productName": "Secrets",
    "version": "0.1.0",
    "description": "Manage encrypted data in S3 using AWS KMS keys",
    "main": "main.js",
    "scripts": {
        "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
        "test-watch": "npm test -- --watch",
        "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
        "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty app test *.js",
        "lint-fix": "npm run lint -- --fix",
        "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
        "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
        "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
        "build": "npm run build-main && npm run build-renderer",
        "start": "cross-env NODE_ENV=production electron ./app/",
        "flow-typed-install": "flow-typed install",
        "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
        "postinstall": "concurrently \"npm run flow-typed-install\" \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
        "dev": "npm run hot-server -- --start-hot",
        "package": "build --publish never",
        "package-win": "build --win --x64",
        "package-linux": "build --linux",
        "package-all": "build -mwl",
        "cleanup": "mop -v"
    },
    "build": {
        "productName": "Secrets",
        "appId": "org.dskyberg.secrets",
        "category": "public.app-category.tools",
        "dmg": {
            "contents": [{
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "files": [
            "dist/",
            "node_modules/",
            "app.html",
            "api/electron_google_oauth.js",
            "main.js",
            "main.js.map",
            "package.json"
        ],
        "win": {
            "target": "nsis"
        },
        "linux": {
            "target": [
                "deb",
                "AppImage"
            ]
        },
        "directories": {
            "buildResources": "resources",
            "output": "release"
        }
    },
    "bin": {
        "electron": "./node_modules/.bin/electron"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dskyberg/secrets.git"
    },
    "author": {
        "name": "David Skyberg",
        "email": "dskyberg@gmail.com",
        "url": "https://github.com/dskyberg"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dskyberg/secrets/issues"
    },
    "keywords": [
        "electron",
        "react",
        "react-router",
        "react-toolbox",
        "mobx",
        "webpack",
        "react-hot",
        "kms",
        "sneaker"
    ],
    "homepage": "https://github.com/dskyberg/secrets#readme",
    "devDependencies": {
        "asar": "^0.12.3",
        "autoprefixer": "^6.5.4",
        "aws-sdk": "^2.7.21",
        "babel-core": "^6.20.0",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^6.2.9",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-dev-expression": "^0.2.1",
        "babel-plugin-tcomb": "^0.3.24",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-plugin-webpack-loaders": "^0.8.0",
        "babel-polyfill": "^6.20.0",
        "babel-preset-env": "^1.1.1",
        "babel-preset-latest": "^6.16.0",
        "babel-preset-react": "^6.16.0",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-react-optimize": "^1.0.1",
        "babel-preset-stage-0": "^6.16.0",
        "babel-register": "^6.18.0",
        "babili-webpack-plugin": "^0.0.8",
        "boiler-room-custodian": "^0.4.2",
        "chai": "^3.5.0",
        "co": "^4.6.0",
        "concurrently": "^3.1.0",
        "cross-env": "^3.1.3",
        "css-loader": "^0.26.1",
        "devtron": "^1.4.0",
        "electron": "1.5.0",
        "electron-builder": "^10.6.1",
        "electron-devtools-installer": "^2.0.1",
        "enzyme": "^2.6.0",
        "eslint": "^3.11.1",
        "eslint-config-airbnb": "^13.0.0",
        "eslint-formatter-pretty": "^1.1.0",
        "eslint-import-resolver-webpack": "^0.7.0",
        "eslint-loader": "^1.6.1",
        "eslint-plugin-babel": "^4.0.0",
        "eslint-plugin-flowtype": "^2.29.1",
        "eslint-plugin-flowtype-errors": "^2.0.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^2.0.1",
        "eslint-plugin-mocha": "^4.7.0",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-react": "^6.8.0",
        "express": "^4.14.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "fbjs-scripts": "^0.7.1",
        "file-loader": "^0.9.0",
        "flow-bin": "^0.37.0",
        "flow-typed": "^2.0.0",
        "googleapis": "^16.1.0",
        "html-webpack-plugin": "^2.24.1",
        "jsdom": "^9.8.3",
        "json-loader": "^0.5.4",
        "minimist": "^1.2.0",
        "mocha": "^3.2.0",
        "node-fetch": "^1.6.3",
        "node-sass": "^4.3.0",
        "path": "^0.12.7",
        "postcss-loader": "^1.2.1",
        "react-addons-test-utils": "^15.4.1",
        "sass-loader": "^4.1.0",
        "sinon": "^1.17.6",
        "spectron": "^3.4.0",
        "style-loader": "^0.13.1",
        "tcomb": "^3.2.15",
        "url-loader": "^0.5.7",
        "webpack": "^1.14.0",
        "webpack-dev-middleware": "^1.8.4",
        "webpack-hot-middleware": "^2.13.2",
        "webpack-merge": "^1.0.2",
        "webpack-validator": "^2.2.11"
    },
    "dependencies": {
        "classnames": "^2.2.5",
        "electron-debug": "^1.1.0",
        "electron-remote": "^1.0.8",
        "electron-require": "^0.3.0",
        "font-awesome": "^4.7.0",
        "history": "^4.5.0",
        "immutability-helper": "^2.0.0",
        "mobx": "^2.7.0",
        "mobx-react": "^4.0.3",
        "mobx-remotedev": "^0.2.4",
        "mobx-store": "^3.4.0",
        "react": "^15.4.1",
        "react-addons-css-transition-group": "^15.4.1",
        "react-dom": "^15.4.1",
        "react-router": "^3.0.0",
        "react-toolbox": "^1.3.4",
        "react-tunnel": "^0.1.0",
        "source-map-support": "^0.4.6",
        "url-parse": "^1.1.7"
    },
    "devEngines": {
        "node": ">=6.x",
        "yarn": ">=0.18.1"
    }
}